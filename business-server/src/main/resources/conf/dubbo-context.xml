<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <context:property-placeholder/>

    <dubbo:registry protocol="nacos" group= "${spring.profiles.active}" address="${dubbo.registry.nacos.address}"/>
    <dubbo:provider timeout="30000" retries="0" filter="providerFilter"></dubbo:provider>
    <dubbo:consumer timeout="25000" retries="0" check="false"></dubbo:consumer>

    <import resource="classpath*:conf/dubbo-consumer.xml" />
    <import resource="classpath*:conf/dubbo-register.xml" />

</beans>